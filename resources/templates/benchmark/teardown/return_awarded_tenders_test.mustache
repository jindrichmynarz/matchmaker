{{!
@param IRI source-graph
@param IRI sample-graph
}}

{{> templates/prefixes.mustache}}

ASK 
WHERE {
  GRAPH <{{sample-graph}}> {
    ?contract pc:awardedTender ?awardedTender .
  }
  FILTER NOT EXISTS {
    GRAPH <{{source-graph}}> {
      ?contract pc:awardedTender ?awardedTender .
    }
  }
}
